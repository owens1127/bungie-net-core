{
    "compilerOptions": {
        "outDir": "./build/",
        "sourceMap": false,
        "strictNullChecks": true,
        "strict": true,
        "module": "esnext",
        "target": "es2017",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "allowJs": true
    },
    "exclude": [
        "src"
    ],
    "include": ["./generator/**/*"]
}