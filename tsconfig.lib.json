{
  "compilerOptions": {
    "target": "es6",
    "module": "ES6",
    "outDir": "./lib",
    "sourceMap": false,
    "strictNullChecks": true,
    "declaration": true,
    "emitDeclarationOnly": false,

    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "incremental": true
  },
  "include": ["./src/*"],
  "exclude": ["node_modules", "**/__tests__/*", "generator", "build"]
}
