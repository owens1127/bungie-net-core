{
  "name": "bungie-net-core",
  "version": "1.0.1",
  "author": "Owen Simpson",
  "homepage": "https://github.com/owensimpson/bungie-net-core",
  "description": "An easy way to interact with the Bungie.net API",
  "keywords": [
    "Destiny 2",
    "Bungie",
    "TypeScript",
    "REST API"
  ],
  "types": "lib/index.d.ts",
  "bugs": "https://github.com/owensimpson/bungie-net-core/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/owensimpson/bungie-net-core"
  },
  "scripts": {
    "create": "git submodule add https://github.com/Bungie-net/api api-src && git submodule init",
    "sync": "git submodule update --remote --merge",
    "clean": "git checkout HEAD -- ./lib ./src ./__test__/api",
    "build": "sh ./build.sh",
    "generate": "sh ./generate.sh",
    "rebuild": "npm run sync && npm run build",
    "format": "prettier --config .prettierrc '{generator,src,test}/**/*.ts' --write",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "checks": "npm run lint && npm test",
    "try": "rm -rf ./node_modules/bungie-net-core && mkdir -p node_modules/bungie-net-core/lib && cp -R ./lib/* ./node_modules/bungie-net-core/lib && cp ./package.json ./node_modules/bungie-net-core/",
    "push": "git push && git push --tags",
    "version": "npm run rebuild && npm run generate && npm run checks && git add -A .",
    "postversion": "npm run push",
    "postpublish": "npm run push && npm i -D bungie-net-core"
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/runtime": "^7.21.0",
    "@jest/globals": "^29.4.3",
    "@types/jest": "^29.4.0",
    "@types/mkdirp": "1.0.0",
    "@types/node": "^16.11.9",
    "@types/underscore": "^1.11.3",
    "bungie-net-core": "^1.0.0",
    "dotenv": "^16.0.3",
    "jest": "^29.4.3",
    "mkdirp": "1.0.3",
    "openapi3-ts": "^2.0.2",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.0-dev.20230422",
    "underscore": "1.13.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "./__test__/tsconfig.test.json"
        }
      ]
    },
    "testMatch": [
      "**/**.test.ts"
    ],
    "globalSetup": "<rootDir>/__test__/global-setup.ts"
  },
  "dependencies": {
    "axios": "^1.3.6"
  }
}
