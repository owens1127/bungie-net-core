{
  "name": "oodestiny",
  "version": "1.1.2",
  "description": "An easy way to interact with the Bungie.net API",
  "keywords": [
    "Destiny 2",
    "Bungie",
    "commonJS",
    "wrapper",
    "REST"
  ],
  "homepage": "https://github.com/owensimpson/oodestiny",
  "type": "module",
  "bugs": "https://github.com/owensimpson/oodestiny/issues",
  "license": "MIT",
  "author": "Owen Simpson",
  "repository": {
    "type": "git",
    "url": "https://github.com/owensimpson/oodestiny"
  },
  "scripts": {
    "build": "sh ./build.sh",
    "publish": "cd lib/ && npm publish",
    "create-submodule": "git submodule add https://github.com/Bungie-net/api api-src",
    "update-spec": "git submodule update --remote --merge"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^16.11.9",
    "@types/node-fetch": "2.5.12",
    "@types/underscore": "^1.11.3",
    "mkdirp": "^1.0.3",
    "node-fetch": "^3.2.10",
    "openapi3-ts": "^2.0.2",
    "typescript": "4.5.2",
    "underscore": "1.13.6"
  }
}
