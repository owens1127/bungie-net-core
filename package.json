{
  "name": "oodestiny",
  "version": "1.7.2",
  "description": "An easy way to interact with the Bungie.net API",
  "keywords": [
    "Destiny 2",
    "Bungie",
    "TypeScript",
    "wrapper",
    "REST"
  ],
  "homepage": "https://github.com/owensimpson/oodestiny",
  "type": "module",
  "bugs": "https://github.com/owensimpson/oodestiny/issues",
  "license": "MIT",
  "author": "Owen Simpson",
  "repository": {
    "type": "git",
    "url": "https://github.com/owensimpson/oodestiny"
  },
  "scripts": {
    "build": "sh ./build.sh",
    "publish": "sh ./publish.sh",
    "create-submodule": "git submodule add https://github.com/Bungie-net/api api-src",
    "update-spec": " git submodule init && git submodule update --remote --merge"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^16.11.9",
    "@types/underscore": "^1.11.3",
    "mkdirp": "^1.0.3",
    "openapi3-ts": "^2.0.2",
    "typescript": "4.5.2",
    "underscore": "1.13.6"
  }
}
